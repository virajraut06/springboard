import java.util.HashMap;
import java.util.Map;

public class HashMapDemo {
	public static void main(String[] args) {
		Map<Integer, String> map =  new HashMap<>();
		map.put(123, "Mumbai");
		map.put(234, "Pune");
		map.put(345, "Nagpur");
		map.put(456, "Chennai");
		map.put(567, "Delhi");
		System.out.println(map);
	}
}
